        {
            "id": "TypedHubFeedCard",
            "type": "GenericShowcard",
            "params": {
                "firstLineMetadata": "{PackageContentsCount} <or> {ChannelNumber} {CallLetters} <or> {PackageContentsCount} <or> {ReleaseYear} | {Rating}",
                "firstLineSelectedMetadata": "{CurrentEpisode} <or> {ChannelNumber} {CallLetters} <or> {PackageContentsCount} <or> {ReleaseYear} | {Rating}"
            }
        },
        {
            "id": "ContinueShowcard",
            "type": "GenericShowcard",
            "params": {
                "ignoreOneClickShowcard": true,
                "replaceInfoLabel": {
                    "type": "VirtualContainer",
                    "params": {
                        "toggle": "oneClickShowcards"
                        },
                        "components": [
                        {
                            "type": "MoreInfoLabel",
                            "className": "info-label",
                            "params": {
                                "parent": ".poster",
                                "html": "<div class='infoText'>{str_details_view_10ft}</div><div class='infoIcon'></div>"
                            }
                        }
                    ]
                },
                "replaceButtons": {
                    "type": "ShowcardButtons",
                    "className": "buttonContainer",
                    "params": {
                        "deferCreation": true
                    },
                    "components": [
                        {
                            "type": "MoreInfoButton",
                            "params": {
                                "events": {
                                    "tap": "onTapOpenDetailsPage"
                                }
                            }
                        },
                        {
                            "type": "ContinueActionButtonGroup",
                            "params": {
                                "events": {
                                    "tap": "onTapOpenPlayOrInfo"
                                }
                            }
                        }
                    ]
                },
                "insertComponentLast": {
                    "type": "Indicators",
                    "params": {
                        "parent": ".metadata > .item-text",
                        "showIndicators": false,
                        "showRecordingIndicators": true
                    }
                }
            }
        },
        {
            "id": "RestartTVShowcard",
            "type": "GenericShowcard",
            "params": {
                "className": "isExtended",
                "firstLineMetadata": "{StartDate} | {StartTime}",
                "firstLineSelectedMetadata": "{EpisodeName} <or> {Name}",
                "bookmarkDefer": true,
                "bookmarkItemMapKey": "",
