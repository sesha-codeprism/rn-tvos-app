{
  "name": "retailclient",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "Open XCWorkSpace": "cd ios && open RetailClient.xcworkspace && cd ..",
    "start": "sh ./pre-build.sh $npm_config_skin && react-native start",
    "postinstall": "patch-package",
