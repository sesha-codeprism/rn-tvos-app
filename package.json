{
  "name": "retailclient",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "Open XCWorkSpace": "cd ios && open RetailClient.xcworkspace && cd ..",
    "Generate TvOS build": "sh MakeIPA.sh",
    "start": "sh ./pre-build.sh $npm_config_skin && react-native start",
    "postinstall": "patch-package",
    "Setup project": "yarn install && cd ios && pod install && cd ..",
    "Setup project for M1 Mac": "yarn install && cd ios && arch -x86_64 pod install && cd ..",
    "Yarn install": "yarn install",
    "Install Pods": "cd ios && pod install && cd ..",
    "Install Pods for M1 Mac": "cd ios && arch -x86_64 pod install && cd ..",
    "Remove Node Modules & Pods": "rm -rf node_modules && rm yarn.lock && rm -rf /ios/Pods && rm /ios/Podfile.lock",
    "test": "jest ",
    "lint": "eslint .",
    "Archive project": "cd ios && xcodebuild archive -workspace RetailClient.xcworkspace -scheme RetailClient-tvOS -archivePath ~/Downloads/MFRetailClient.xcarchive | xcpretty",
    "Generate IPA from Archive": "cd ios && xcodebuild -exportArchive -archivePath ~/Downloads/MFRetailClient.xcarchive -exportPath ~/Downloads -exportOptionsPlist ExportOptions.plist -allowProvisioningUpdates | xcpretty"
  },
  "engines": {
    "node": "<17",
    "npm": "<8.2"
  },
  "dependencies": {
    "@react-navigation/drawer": "^6.4.1",
