    "patch-package": "^6.4.7",
    "postinstall-postinstall": "^2.1.0",
    "react": "17.0.1",
    "react-native": "npm:react-native-tvos@0.64.2-2",
    "react-native-device-info": "^8.4.8",
    "react-native-fast-image": "^8.6.1",
    "react-native-gesture-handler": "^2.4.2",
    "react-native-i18n": "^2.0.15",
